<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables continueOnError="false" enabled="true" name="Get-Variables">
    <DisplayName>Get-Variables</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="pushEndpoint">
            <JSONPath>$.pushConfig.pushEndpoint</JSONPath>
        </Variable>
        <Variable name="serviceAccountEmail">
            <JSONPath>$.pushConfig.oidcToken.serviceAccountEmail</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">request</Source>
</ExtractVariables>